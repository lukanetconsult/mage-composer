{
    "name" : "luka/mage-composer-plugin",
    "type" : "application",
    "repositories" : [
        { "type" : "git", "url" : "https://github.com/composer/composer" }
    ],
    "require" : {
        "composer/composer" : "dev-master"
    },
    "autoload" : {
        "psr-0" : {
            "luka\\composer\\" : "src/"
        },
        "classmap" : [ "src/Composer/" ]
    },
    "description" : "This plugin allows installing packages from magento connect",
    "homepage" : "http://www.luka.de/",
    "license" : "GPL-3.0+",
    "authors" : [{
            "name" : "Axel Helmert",
            "role" : "Lead Developer"
        }
    ],
    "keywords" : [
        "magento",
        "connect"
    ]
}
